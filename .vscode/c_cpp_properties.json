{
    "configurations": [
        {
            "name": "macOS GCC Amiga",
            "includePath": [
                "/Volumes/repos/amigagpt/**",
                "/opt/amiga/m68k-amigaos/include",
                "/opt/amiga/m68k-amigaos/ndk-include",
                "/opt/amiga/m68k-amigaos/libnix/include",
                "/opt/amiga/m68k-amigaos/sys-include",
                "/opt/amiga/m68k-amigaos/clib2/include"
                // "/Volumes/repos/vcpkg/installed/arm64-osx/include"
            ],
            "defines": [],
            "macFrameworkPath": [
                "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "macos-gcc-arm64",
            "mergeConfigurations": false,
            "compilerPath": "/opt/amiga/bin/m68k-amigaos-gcc",
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}